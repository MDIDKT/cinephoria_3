        </tbody>
    </table>

    <a href="{{ path('app_admin_place_index') }}">back to list</a>

    <a href="{{ path('app_admin_place_edit', {'id': place.id}) }}">edit</a>

    {{ include('admin_place/_delete_form.html.twig') }}
{% endblock %}
