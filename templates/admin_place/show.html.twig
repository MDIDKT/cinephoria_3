{% extends 'base.html.twig' %}

{% block title %}Place{% endblock %}

{% block body %}
    <h1>Place</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ place.id }}</td>
            </tr>
            <tr>
                <th>Siege</th>
                <td>{{ place.siege }}</td>
            </tr>
            <tr>
                <th>SiegeMobiliteReduite</th>
                <td>{{ place.siegeMobiliteReduite ? 'Yes' : 'No' }}</td>
